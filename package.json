    "butter-resources": "^3.15.0",
    "butter-share": "^3.15.1",
    "butter-styles": "^3.15.0",
    "butter-vue-page-swiper": "^3.15.0",
    "butter-vue-pay": "^3.15.1",
    "butter-vue-widgets": "^3.15.1",
    "buttercam": "^3.15.0",
    "clean-webpack-plugin": "^0.1.19",
    "clipboard": "^2.0.1",
