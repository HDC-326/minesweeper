{
  "name": "minesweeper",
  "author": "HDC",
  "version": "0.0.1",
  "description": "minesweeper",
  "browserslist": [
    "chrome >= 30",
    "safari >= 8",
    "iOS >= 8",
    "android >= 4.4",
    "ie > 10"
  ],
  "scripts": {
    "test": "jest",
    "update-api": "ba:gateway",
    "dev": "NODE_ENV=development DEBUG=Dev node bin/dev",
    "dev:win": "set NODE_ENV=development&&set DEBUG=Dev&&node bin/dev",
    "build:pro": "NODE_ENV=production API_LEVEL=production DEBUG=Dist node bin/dist",
    "build:pro:win": "set NODE_ENV=production&&set API_LEVEL=production&&set DEBUG=Dist&&node bin/dist",
    "build:dev": "NODE_ENV=development API_LEVEL=development DEBUG=Dist node bin/dist",
    "build:dev:win": "set NODE_ENV=development&&set API_LEVEL=development&&set DEBUG=Dist&&node bin/dist",
    "build:anlz": "NODE_ENV=production API_LEVEL=production DEBUG=Analyze node bin/analyze",
    "build:anlz:win": "set NODE_ENV=production&&set API_LEVEL=production&&set DEBUG=Analyze&&node bin/analyze",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-jest": "^23.4.2",
    "body-parser": "^1.18.3",
    "eslint": "^4.19.1",
    "eslint-plugin-vue": "^4.5.0",
    "express": "^4.16.3",
    "ip": "^1.1.5",
    "jest": "^23.5.0",
    "morgan": "^1.9.0",
    "vue-hot-reload-api": "^2.3.0",
    "vue-jest": "^2.6.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-dev-server": "^3.1.6",
    "webpack-hot-middleware": "^2.22.2"
  },
